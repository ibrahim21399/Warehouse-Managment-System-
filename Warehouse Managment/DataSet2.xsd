                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Warehouse.Warhouse_Name AS [WarehouseName], Product.prod_Name AS [ProductName], Sup_Requst_Detailes.IQuntity AS [Quantity], suppliers.Sup_Name AS [person], 
	SupPermission.Prem_Date AS [PermissionDate] ,(select 'Import Products')as [Type]
	
FROM SupPermission INNER JOIN
    Sup_Requst_Detailes ON SupPermission.prem_Num =Sup_Requst_Detailes.perem_Num INNER JOIN
    suppliers ON suppliers.Sup_Id = SupPermission.Sup_Id INNER JOIN
    Warehouse ON Warehouse.Warhouse_Id = Sup_Requst_Detailes.War_Id INNER JOIN
 
   Product ON Product.prod_Id =  Sup_Requst_Detailes.Prod_Id 
where 
 Warehouse.Warhouse_Name in (select * from string_split(@w_name,','))
and
 SupPermission.Prem_Date between @date1 and @date2 
and Product.prod_Name=@prdnsme


union
	SELECT 
	Warehouse.Warhouse_Name AS [WarehouseName],
	Product.prod_Name AS [ProductName], Cus_Requst_Detailes.OQuntity AS [Quantity], Customers.Cus_Name AS [person],  
	CusPermission.Prem_Date AS [PermissionDate],(select 'withdrawl Products' )as [Type] 
	FROM CusPermission INNER JOIN
    Cus_Requst_Detailes ON CusPermission.prem_Num =Cus_Requst_Detailes.perem_Num INNER JOIN
    Customers ON Customers.Cus_Id = CusPermission.Cus_Id INNER JOIN
    Warehouse ON Warehouse.Warhouse_Id = Cus_Requst_Detailes.War_Id   INNER JOIN
    Product ON Product.prod_Id =  Cus_Requst_Detailes.Prod_Id where
 Warehouse.Warhouse_Name in (select * from string_split(@w_name,','))
and
 CusPermission.Prem_Date between @date1 and @date2 and Product.prod_Name=@prdnsme</CommandText>
